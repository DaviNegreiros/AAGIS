</head>

<body>
    <div class="sidebar">
        <div class="top">
            <div class="logo">
                <i class='bx bx-home-alt-2'></i>
                <span>AAGIS</span>
            </div>
            <i class="bx bx-menu" id="btn"></i>
        </div>

        {{#if user_logado}}
        <div class="user"><a href="/perfil">
        <div class="pfpContainer"><img src={{user_foto}} alt="me" class="user-img"> </div>
            </a>
            <div>
                <p class="bold">{{user_name}}</p>
                <p>{{user_tipo}}</p>
            </div>
        </div>
        {{/if}}
        {{#unless user_logado}}
        <div class="user">
            <a href="/perfil">
            <div class="pfpContainer"><img
                    src="https://static-00.iconduck.com/assets.00/profile-user-icon-2048x2048-m41rxkoe.png" alt="me"
                    class="user-img"></div>
            </a>
            <div>
                <p class="bold">{{user_name}}</p>
                <p>{{user_tipo}}</p>
            </div>
        </div>
        {{/unless}}

        <ul>
            <li>
                <a href="/calendario">
                    <i class="bx bxs-grid-alt"></i>
                    <span class="nav-item">Calendário</span>
                </a>
                <span class="tooltip">Calendário</span>
            </li>
            <li id="btnFiltro">
                <a href="#">
                    <i class="bx bx-list-check"></i>
                    <span class="nav-item">Filtro</span>
                </a>
                <span class="tooltip">Filtro</span>
            </li>

            <!-- Formulário para filtros -->
            <form id="filtroForm" action="/" method="get">
                <fieldset class="filtro">
                    <label class="filtro1" onclick="ck1()">
                        <input id="cb1" type="checkbox" name="curso" value="Todos" checked>
                        Todos
                        <span class="checkmark"></span>
                    </label>

                    <label class="filtro1" onclick="ck2()">Engenharias
                        <input id="cb2" type="checkbox" name="curso" value="Engenharias">
                        <span class="checkmark"></span>
                    </label>

                    <label class="filtro1" onclick="ck3()">Engenharia de Software
                        <input id="cb3" type="checkbox" name="curso" value="Eng. Software">
                        <span class="checkmark"></span>
                    </label>

                    <label class="filtro1" onclick="ck4()">Engenharia de Energia
                        <input id="cb4" type="checkbox" name="curso" value="Eng. Energia">
                        <span class="checkmark"></span>
                    </label>

                    <label class="filtro1" onclick="ck5()">Engenharia Aeroespacial
                        <input id="cb5" type="checkbox" name="curso" value="Eng. Aeroespacial">
                        <span class="checkmark"></span>
                    </label>

                    <label class="filtro1" onclick="ck6()">Engenharia Automotiva
                        <input id="cb6" type="checkbox" name="curso" value="Eng. Automotiva">
                        <span class="checkmark"></span>
                    </label>

                    <label class="filtro1" onclick="ck7()">Engenharia Eletrônica
                        <input id="cb7" type="checkbox" name="curso" value="Eng. Eletrônica">
                        <span class="checkmark"></span>
                    </label>
                    <button type="submit" id="botaoAplicarFiltros">Aplicar Filtros</button>
                </fieldset>

            </form>

            {{#if user_logado}}
            <li>
                <a onclick="rotaPost()">
                    <i class='bx bx-plus-medical' onclick="rotaPost()"></i>
                    <span class="nav-item" onclick="rotaPost()">Publicar</span>
                </a>
                <span class="tooltip">Publicar</span>
            </li>

            {{#if user_adm}}
            <li>
                <a href="/controle">
                    <i class='bx bxs-cog'></i>
                    <span class="nav-item">Controle</span>
                </a>
                <span class="tooltip">Controle</span>
            </li>
            <li>
                <a href="/painel">
                    <i class='bx bx-list-ol'></i>
                    <span class="nav-item">Painel</span>
                </a>
                <span class="tooltip">Painel</span>
            </li>
            {{/if}}

            <li>
                <a href="/logout">
                    <i class="bx bx-log-out"></i>
                    <span class="nav-item">Logout</span>
                </a>
                <span class="tooltip">Logout</span>
            </li>
            {{/if}}

            {{#unless user_logado}}
            <li>
                <a href="/login">
                    <i class='bx bx-log-in'></i>
                    <span class="nav-item">Login</span>
                </a>
                <span class="tooltip">Login</span>
            </li>
            {{/unless}}
        </ul>
    </div>


    <div class="header">
        <div class="HeaderRow">
            <div class="home" onclick="homeFunction()">
                <h1 id="HOME">AAGIS</h1>
                <div id="imgPadding">
                    <img src="imagens/unb_logo.png" alt="Logo UnB">
                </div>
            </div>
        </div>

        <a id="slogan">Atualizações Acadêmicas Gerais, Indispensáveis e Sucintas</a>

    </div>
    <div class="main">
